{
  "version": 2,
  "buildCommand": "cd Generative/frontend && npm run build",
  "outputDirectory": "Generative/frontend/build",
  "framework": "create-react-app",
  "functions": {
    "api/main.py": {
      "runtime": "@vercel/python@4.3.0"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/main.py"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}